    "**zsh**": "shellscript",
    "*.aux": "latex",
    "*.bbl": "latex",
    "*.bcf": "latex",
    "*.bib": "latex",
    "*.blg": "latex",
    "*.bst": "latex",
    "*.gs": "javascript",
    "*.json": "jsonc",
    "*.lbx": "latex",
    "*.out": "latex",
    "*github": "git",
    "*gitlab": "git",
    ".zcompdump": "log",
    ".zsh_history": "log",
    "DIR_COLORS": "shellscript",
    "apt_config": "shellscript",
    "apt_configure-index": "shellscript",
    "bibs": "bibtex",
    "colorenv": "shellscript",
    "common-repos": "git",
    "config": "properties",
    "credentials": "properties",
    "dircolors": "shellscript",
    "env": "shellscript",
    "envfunctions": "shellscript",
    "envlast": "shellscript",
    "gitcolor": "git",
    "gitconfig": "git",
    "mysudoers": "shellscript",
    "ssh-config": "properties",
    "sudo-askpass": "shellscript",
    "vscode": "properties",
    "wgetrc": "properties",
    "wt": "properties",
    "zi-colors": "shellscript"
